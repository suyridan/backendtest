    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^2.0.22",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "preset": "ts-jest",
    "coveragePathIgnorePatterns": [
