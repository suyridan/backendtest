    "dotenv": "^16.3.1",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "memory-cache": "^0.2.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
